{
  "expo": {
    "name": "Dinspo: Date Ideas",
    "slug": "Dinspo-Date-Ideas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/iphone-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5BD3BD"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vindhyavali.DinspoDateIdeas"
    },
    "android": {
      "googleServicesFile": "./app/assets/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vindhyavali.DinspoDateIdeas",
      "versionCode": 2,
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "18e366dd-fd3e-479a-9c9e-b502139951f1"
      }
    }
  }
}
